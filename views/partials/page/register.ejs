<h1>Register</h1>
 <form class="register" method="post" action="/register"> 
  <fieldset class="form-group">
    <label for="username">Username</label>
    <input type="text" name="login" class="form-control" placeholder="Username">
  </fieldset>
  <fieldset class="form-group">
    <label for="email">Email</label>
    <input type="text" name="email" class="form-control" placeholder="Email">
  </fieldset>
  <fieldset class="form-group">
    <label for="password">Password</label>
    <input type="password" name="password" class="form-control" placeholder="Password">
  </fieldset>
  <fieldset class="form-group">
    <label for="passwordConfirmation">Confirm Password</label>
    <input type="password" name="passwordConfirmation" class="form-control" placeholder="Confirm Password">
  </fieldset>
  <fieldset class="form-group">
    <label for="dob">DOB</label>
    <input type="text" name="dob" class="form-control" placeholder="dd/mm/yyyy">
  </fieldset>
  <fieldset class="form-group">
    <label for="preferredZen">Preferred Zen</label>
    <input type="text" name="preferredZen" class="form-control" placeholder="Preferred Zen">
  </fieldset>
  <fieldset class="form-group">
      <label for="genderDropDown">Gender</label>
      <select multiple class="form-control" id="exampleSelect1" name="Gende r">
        <option>Male</option>
        <option>Female</option>
      </select>
    </fieldset>
    <label for="avatar"></label>
    <input type="text" name="avatar" class="form-control" placeholder="avatar">
  </fieldset>
<button type="submit" class="btn btn-default">Submit</button>
</form>
